create table Valider(id_diplome int, student_id int,num_session int,note float not null,moy float not null,constraint pk_valider primary key (id_diplome,student_id,num_session),constraint fk_valider_diplome foreign key (id_diplome) references diplome (id_diplome) on delete set cascade on update cascade,constraint fk_valider_etudiant foreign key (student_id) references etudiant (student_id) on delete set null on update cascade,constraint fk_valider_session foreign key (num_session) references session (num_session) on delete set null on update cascade);